{
  "name": "cesarfelip3/laravel",
  "description": "Laravel Kickstart Project",
  "keywords": [
    "framework",
    "laravel",
    "SLC DevShop"
  ],
  "version": "1.0.0",
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/SLCDevShop/laroute"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "artesaos/defender": "dev-master@dev",
    "fideloper/proxy": "~3.3",
    "laravel/cashier": "7.0.*",
    "laravel/dusk": "^1.1",
    "laravel/framework": "5.5.*",
    "laravel/passport": "3.0.*",
    "laravel/scout": "3.0.*",
    "laravel/tinker": "~1.0",
    "lord/laroute": "dev-master",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "2.4.*",
    "barryvdh/laravel-ide-helper": "2.4.*",
    "bugsnag/bugsnag-laravel": "2.6.*",
    "deployer/deployer": "^5.1",
    "deployer/recipes": "^4.0",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "pdepend/pdepend": "2.5.*",
    "phploc/phploc": "4.0.*",
    "phpmd/phpmd": "2.6.*",
    "phpmetrics/phpmetrics": "2.2.*",
    "phpunit/phpunit": "~6.0",
    "sebastian/phpcpd": "3.0.*",
    "squizlabs/php_codesniffer": "3.0.*",
    "theseer/phpdox": "0.10.*"
  },
  "autoload": {
    "files": [
      "app/Helpers/General.php"
    ],
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate",
      "@php artisan storage:link",
      "@php artisan clear-compiled",
      "@php artisan ide-helper:generate",
      "@php artisan optimize",
      "php artisan project:setup"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan optimize",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
